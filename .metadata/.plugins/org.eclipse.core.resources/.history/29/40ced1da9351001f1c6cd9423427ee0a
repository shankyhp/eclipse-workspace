		int x = 46;
		int y = 23;
		int temp;
		
		temp=x;
		x=y;
		y=temp;
		
		
		
		System.out.println("Enter your name");
		String name = scanner.nextLine();
		
		
		
		System.out.println("Enter any digit you like");
		int digit = scanner.nextInt();
		
		
		System.out.println("HEllo MR :"+name);
		System.out.println("YOUR DIGIT IS '+digit'"+temp);
		
		
		
	}

}
