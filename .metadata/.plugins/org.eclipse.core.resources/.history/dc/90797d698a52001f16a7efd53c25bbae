package myfirst;

public class MethodEx2 {

	public static void main(String[] args) {
	
		
		
		int x =4;
		int y=2;
		
	//	int z =add(x,y);
	//	System.out.println(z);
		System.out.println(add(x,y));
		
	}

	//return value is to be in integer so int used 
	static int add(int x,int y)
	{

		int z= x +y;
		return z;

}
}
